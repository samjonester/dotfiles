{
  "workbench.colorTheme": "Solarized Dark",
  "workbench.preferredDarkColorTheme": "Solarized Dark",
  "workbench.preferredLightColorTheme": "Solarized Light",
  "window.autoDetectColorScheme": true,
  "editor.fontFamily": "Source Code Pro",
  "editor.codeLensFontFamily": "Source Code Pro",
  "editor.formatOnSave": true,
  "editor.multiCursorModifier": "alt",
  "editor.stickyScroll.enabled": true,
  "git.enableCommitSigning": true,
  "git.enableSmartCommit": true,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "files.trimTrailingWhitespace": true,
  "files.trimTrailingWhitespaceInRegexAndStrings": true,
  "editor.rulers": [120],
  "files.autoSave": "afterDelay",
  "terminal.external.profiles.osx": {
    "bash": {
      "path": "bash",
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh",
      "icon": "terminal-zsh",
      "args": ["-i", "-l"]
    }
  },
  "terminal.integrated.defaultProfile.osx": "zsh",
  "security.workspace.trust.untrustedFiles": "open",
  "cSpell.userWords": ["autofetch", "println", "samjonester", "signup"],
  "github.copilot.preferredAccount": "samjonester",
  "remote.SSH.connectTimeout": 150,
  "editor.fontSize": 14,
  "chat.editor.fontSize": 14,
  "terminal.integrated.fontSize": 14,
  "debug.console.fontSize": 14,
  "git.autofetch": true,
  "gitlens.ai.experimental.generateCommitMessage.enabled": false
}
